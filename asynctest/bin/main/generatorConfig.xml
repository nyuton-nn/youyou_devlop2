<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="context1"  targetRuntime="MyBatis3">
    <property name="autoDelimitKeyWords" value="true" />
    <property name="beginningDelimiter" value="`" />
    <property name="endingDelimiter" value="`" />
    <jdbcConnection 
    	connectionURL="jdbc:mysql:///" 
    	driverClass="com.mysql.jdbc.Driver" 
    	password="" 
    	userId="" />
    <javaModelGenerator 
    	targetPackage="asynctest.model" 
    	targetProject="asynctest/src/main/java">
	    <property name="enableSubPackages" value="true" />
    	<property name="trimStrings" value="true" />
    </javaModelGenerator>
    <sqlMapGenerator
    	targetPackage="asynctest.mapper"
    	targetProject="asynctest/src/main/resources">
    	<property name="enableSubPackages" value="true" />
	</sqlMapGenerator>
    <javaClientGenerator 
    	targetPackage="asynctest.mapper" 
    	targetProject="asynctest/src/main/java" 
    	type="XMLMAPPER" >
	    <property name="enableSubPackages" value="true" />
    </javaClientGenerator> 	
    <table  tableName="KABU_ALL_T" domainObjectName="KabuAllModel"
            mapperName="KabuAllMapper"
            enableInsert="true"
            enableSelectByPrimaryKey="true"
            enableSelectByExample="true"
            enableUpdateByPrimaryKey="true"
            enableUpdateByExample="true"
            enableDeleteByPrimaryKey="true"
            enableDeleteByExample="true"
            enableCountByExample="true"
            selectByExampleQueryId="true"
            modelType="flat" >
            <columnRenamingRule searchString="^class$" replaceString="clazz"/>
    </table>
    <table  tableName="MT_USER" domainObjectName="MtUserModel"
            mapperName="MtUserMapper"
            enableInsert="true"
            enableSelectByPrimaryKey="true"
            enableSelectByExample="true"
            enableUpdateByPrimaryKey="true"
            enableUpdateByExample="true"
            enableDeleteByPrimaryKey="true"
            enableDeleteByExample="true"
            enableCountByExample="true"
            selectByExampleQueryId="true"
            modelType="flat" >
            <columnRenamingRule searchString="^class$" replaceString="clazz"/>
    </table>
  </context>
</generatorConfiguration>
